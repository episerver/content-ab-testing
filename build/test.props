<Project>
  <PropertyGroup>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
    <GenerateDocumentationFile>false</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="$(microsofttestsdk)" />
    <PackageReference Include="xunit.runner.visualstudio" Version="$(xunitrunnervisualstudioVersion)" />
    <PackageReference Include="xunit.core" Version="$(xunitcore)"  />
    <PackageReference Include="xunit.assert" Version="$(xunitassert)" />
    <PackageReference Include="xunit.analyzers" Version="$(xunitanalyzers)" />
    <PackageReference Include="Moq" Version="$(MoqVersion)" />
  </ItemGroup>
  
</Project>